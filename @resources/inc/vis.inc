
[MeasureSamplingRate]
Measure=Plugin
Plugin=AudioLevel
Type=Format
RegExpSubstitute=1
Substitute="(Hz.*)$":""
UpdateDivider=-1

[mAudio]
measure=plugin
plugin=audiolevel
port=output
FFTSize=(Floor([MeasureSamplingRate] * #accuracy# / #FPS#))
FFTOverlap=(Floor([MeasureSamplingRate] * (#accuracy# - 1) / #FPS#))
fftattack=0
fftdecay=50
bands=14
freqmin=20
freqmax=16500
sensitivity=#sensitivity#
disabled=(1-#visDisp#)
dynamicvariables=1

[b1]
measure=plugin
plugin=audiolevel
parent=mAudio
type=band
averagesize=#avgSize#
disabled=(1-#visDisp#)
bandidx=2
